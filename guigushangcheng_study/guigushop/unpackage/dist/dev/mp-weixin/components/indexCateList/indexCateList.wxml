<view class="cateListContainer"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{indexCateItem.category.bannerUrlList}}" wx:for-item="bannerItem" wx:for-index="__i0__" wx:key="*this"><swiper-item><view class="swiper-item"><image src="{{bannerItem}}" mode></image></view></swiper-item></block></swiper><view class="common">{{indexCateItem.category.name}}</view><view class="common">{{indexCateItem.category.frontDesc}}</view><view class="shopList"><block wx:for="{{indexCateItem.itemList}}" wx:for-item="shopItem" wx:for-index="__i1__" wx:key="id"><view class="shopItem"><image src="{{shopItem.listPicUrl}}" mode></image><view class="shopInfo">{{shopItem.name}}</view><view class="price">{{"$ "+shopItem.retailPrice}}</view></view></block></view></view>