.cateListContainer swiper {
  width: 100%;
  height: 370rpx;
}
.cateListContainer swiper .swiper-item {
  width: 100%;
  height: 100%;
}
.cateListContainer swiper .swiper-item image {
  width: 100%;
  height: 100%;
}
.cateListContainer .shopList {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-justify-content: space-around;
      -ms-flex-pack: distribute;
          justify-content: space-around;
}
.cateListContainer .shopList:after {
  content: '';
  width: 345rpx;
  height: 0;
}
.cateListContainer .shopList .shopItem {
  width: 345rpx;
}
.cateListContainer .shopList .shopItem image {
  width: 345rpx;
  height: 345rpx;
}
.cateListContainer .shopList .shopItem .shopInfo {
  font-size: 28rpx;
}
.cateListContainer .shopList .shopItem .price {
  color: #bb2c08;
  font-size: 26rpx;
}
.cateListContainer .common {
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
}

